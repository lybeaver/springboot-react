<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <classPathEntry location="C:\Users\osd\.m2\repository\mysql\mysql-connector-java\5.1.47\mysql-connector-java-5.1.47.jar" />

  <context id="DB2Tables" targetRuntime="MyBatis3">
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://localhost:3306/lin?useUnicode=true&amp;characterEncoding=utf-8"
        userId="root"
        password="root">
    </jdbcConnection>

    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <javaModelGenerator targetPackage="com.example.mydemo.beans" targetProject="src/main/java">
      <property name="constructorBased" value="true"/>
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="mapper"  targetProject="src/main/resources">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="com.example.mydemo.mapper"  targetProject="src/main/java">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>

    <table schema="DB2ADMIN" tableName="t_user" domainObjectName="tUser" >
      <property name="useActualColumnNames" value="false"/>
      <generatedKey column="ID" sqlStatement="DB2" identity="true" />
    </table>
	
	<table schema="DB2ADMIN" tableName="t_user_role" domainObjectName="tUserRole" >
      <property name="useActualColumnNames" value="false"/>
    </table>
    
    <table schema="DB2ADMIN" tableName="t_role" domainObjectName="tRole" >
      <property name="useActualColumnNames" value="false"/>
      <generatedKey column="ID" sqlStatement="DB2" identity="true" />
    </table>
    
    <table schema="DB2ADMIN" tableName="t_resource" domainObjectName="tResource" >
      <property name="useActualColumnNames" value="false"/>
      <generatedKey column="ID" sqlStatement="DB2" identity="true" />
    </table>
    
    <table schema="DB2ADMIN" tableName="t_permission" domainObjectName="tPermission" >
      <property name="useActualColumnNames" value="false"/>
      <generatedKey column="ID" sqlStatement="DB2" identity="true" />
    </table>
    
    <table schema="DB2ADMIN" tableName="t_role_resource_permission" domainObjectName="tRoleResourcePermission" >
      <property name="useActualColumnNames" value="false"/>
    </table>
  </context>
</generatorConfiguration>